#: cookieadmin/assets/cookie/policies.php:33
#: cookieadmin/assets/cookie/policies.php:68
msgid "Reject All"
msgstr "Rifiuta Tutto"

#: cookieadmin/assets/cookie/policies.php:36
#: cookieadmin/assets/cookie/policies.php:71
msgid "Accept All"
